<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="oval" android:innerRadius="150px" >
            <solid android:color="#fff"/>

            <stroke
                android:width="5px"
                android:color="#4B4B4B" />
        </shape>
    </item>
    <item android:top="10px" android:bottom="10px" android:left="10px" android:right="10px">
        <shape android:shape="oval">
            <solid android:color="#4B4B4B" />

            <stroke
                android:width="1px"
                android:color="#4B4B4B" />
        </shape>
    </item>

</layer-list>